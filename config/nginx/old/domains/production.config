# both grantdash and custom domain
location /dash {
  root /home/web/projects/grantdash/server/static/;

  location ~ ^/dash/work/(.*)$ {
    default_type text/plain;
    return 404 " ";
  }
  location ~ ^/dash/misc/(.*)$ {
    default_type text/plain;
    return 404 " ";
  }

  include ./sites-enabled/grantdash.io/modules/dash.config;
}

include ./sites-enabled/grantdash.io/modules/proxy.config;
include ./sites-enabled/grantdash.io/modules/https.config;

